{
    "extends": "airbnb",
    "rules": {
        "indent": [
            1,
        4
        ],
        "quotes": [
            0,
            "double"
        ],
        "no-unused-vars": 1,
        "no-var": 0,
        "space-before-blocks": 0,
        "camelcase": 0,
        "prefer-const": 0,
        "no-shadow": 0,
        "func-names": 0,
        "vars-on-top": 0,
        "new-cap": 0,
        "guard-for-in": 0,
        "space-after-keywords": 0,
        "space-before-function-paren": 0,
        "no-param-reassign": 0,
        "no-else-return": 0,
        "comma-spacing": 0,
        "brace-style": 0,
        "no-multi-spaces": 0,
        "no-spaced-func": 0,
        "react/sort-comp": 0,
        "react/prop-types": 0,
    },
}
